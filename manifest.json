{
    "name": "Group Contacts Download",
    "version": "1.4",
    "description": "This is an unofficial enhacement for WhatsApp Web. This extension in any way is not related to WhatsApp Inc. WhatsApp is a trademark of WhatsApp Inc.",
    "manifest_version": 2,
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action":{
        "default_icon": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://web.whatsapp.com/"
            ],
            "js": [
                "resources/jquery-3.3.1.min.js",
				"CSV_creator.js",
                "content.js"
            ]
        }
    ],
    "permissions": [
        "tabs"
    ]
}